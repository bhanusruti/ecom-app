<table class="table">
  <tr>
      <th>Name</th>
      <th>Price</th>
      <th>Description</th>
      <th>Image</th>
  </tr>

  <% @products.each do|product| %>
  <tr>
      <td><%= product.name %></td>
      <td><%= product.price %></td>
      <td><%= product.description %></td>
      <td><img class="img-responsive"src=<%= product.image %>></td>
  </tr>
  <%end%>
</table>